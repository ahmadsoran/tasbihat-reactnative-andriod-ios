{
  "expo": {
    "name": "tasbihat",
    "slug": "tasbihat",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/icon.png",
    "scheme": "tasbihat",
    "userInterfaceStyle": "automatic",
    "description": "tasbih Counting, quran Text & audio, 3 language support & more",
    "privacy": "public",
    "githubUrl": "https://github.com/ahmadsoran",
    "owner": "ahmadsorann",
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/img/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#00b85c"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.ahmedsoran.tasbihat",
      "buildNumber": "2.0.0",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/adaptive-icon.png",
        "backgroundColor": "#00b85c"
      },
      "package": "com.ahmedsoran.tasbihat",
      "versionCode": 2,
      "permissions": [
        "INTERNET",
        "VIBRATE"
      ],
      "useNextNotificationsApi": true
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you set your custom wallpaper"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/img/adaptive-icon.png",
          "color": "#00b85c",
          "sounds": [
            "./assets/adthan/athan_1.mp3",
            "./assets/adthan/athan_2.mp3",
            "./assets/adthan/athan_3.mp3",
            "./assets/adthan/athan_4.mp3",
            "./assets/adthan/athan_5.mp3",
            "./assets/adthan/athan_6.mp3",
            "./assets/adthan/athan_7.mp3"
          ]
        }
      ]
    ],
    "web": {
      "favicon": "./assets/img/favicon.png"
    }
  }
}